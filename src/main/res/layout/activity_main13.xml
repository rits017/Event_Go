<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#070400"
    android:backgroundTint="#070707"
    tools:context=".MainActivity13">

    <TextView
        android:id="@+id/tv131"
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:paddingLeft="30dp"
        android:paddingTop="10dp"
        android:text="Search Your Event Here"
        android:textColor="#E91E63"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <EditText
        android:id="@+id/ed131"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="#F44336"
        android:ems="10"
        android:hint="Enter Event Name"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColor="#ece8e2"
        android:textColorHint="#ece8e2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.604" />

    <EditText
        android:id="@+id/ed132"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="#03A9F4"
        android:ems="10"
        android:hint="Enter Event ID"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColor="#ece8e2"
        android:textColorHint="#ece8e2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.716" />

    <Button
        android:id="@+id/b131"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@drawable/roundbutton2"
        android:backgroundTint="#36D811"
        android:text="SEARCH"
        android:textColor="#ece8e2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.866" />

    <ImageView
        android:id="@+id/iv131"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.193"
        app:srcCompat="@drawable/ev9" />

</androidx.constraintlayout.widget.ConstraintLayout>